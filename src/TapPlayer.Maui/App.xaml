        <ResourceDictionary Source="Resources/Styles/ColorPaletteDark.xaml" />
        <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
        <ResourceDictionary Source="Resources/Styles/TileColorPalette.xaml" />
        <ResourceDictionary Source="Resources/Styles/TileColorPaletteDark.xaml" />
        <ResourceDictionary Source="Resources/Styles/TileButtons.xaml" />
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
</Application>
